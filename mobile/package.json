{
  "name": "expo-boilerplate",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "db": "json-server --watch db.json --port 8081",
    "android": "expo run:android",
    "test": "jest",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "check-typescript": "tsc --noEmit",
    "check-eslint": "eslint './src/**/*{js,ts,jsx,tsx}'",
    "lint": "yarn check-typescript && yarn check-eslint",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.22.5",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-picker/picker": "2.4.10",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@shopify/flash-list": "1.4.3",
    "@types/react": "~18.2.14",
    "@types/react-native": "^0.72.2",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "expo": "^49.0.0",
    "expo-auth-session": "~5.0.2",
    "expo-build-properties": "~0.8.3",
    "expo-crypto": "~12.4.1",
    "expo-dev-client": "~2.4.12",
    "expo-font": "~11.4.0",
    "expo-navigation-bar": "~2.3.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-web-browser": "~12.3.2",
    "firebase": "^10.4.0",
    "json-server": "^0.17.3",
    "lottie-react-native": "5.1.6",
    "native-base": "^3.4.28",
    "react": "18.2.0",
    "react-hook-form": "^7.45.1",
    "react-native": "0.72.6",
    "react-native-dotenv": "^3.4.9",
    "react-native-pager-view": "6.2.0",
    "react-native-picker-select": "^9.0.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-query": "^3.39.3",
    "reactotron-react-native": "^5.0.3",
    "typescript": "^5.1.3",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-syntax-jsx": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "metro-react-native-babel-preset": "^0.76.6",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix --ext .tsx --ext .ts"
    ]
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "private": true
}
